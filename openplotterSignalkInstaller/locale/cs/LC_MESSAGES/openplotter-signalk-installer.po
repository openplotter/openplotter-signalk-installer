#: connections.py:70 connections.py:85
msgid "The access request must be aproved with read/write permission in Signal K administrator."
msgstr "Žádost o přístup musí být potvrzena oprávněním ke čtení/zápisu v administraci Signal K."

#: connections.py:73
msgid "Failed access request, status code: "
msgstr "Žádost o přístup selhala, status kódu: "

#: connections.py:76
msgid "Error requesting access to Signal K server: "
msgstr "Došlo k chybě při zaslání požadavku o přístup serveru Signal K: "

#: connections.py:83
msgid "The access request no longer exists."
msgstr "Požadavek na přístup již neexistuje."

#: connections.py:91
msgid "The access request was denied."
msgstr "Přístup byl odmítnut."

#: connections.py:101 connections.py:109
msgid "Set read/write permission for this access in Signal K administrator."
msgstr "Nastavte přístup \"read/write\" pro připojení k serveru Signal K v administraci."

#: connections.py:102
msgid "The access request was successfully approved."
msgstr "Žádost o přístup byla úspěšně schválena."

#: connections.py:116
msgid "Unexpected error validating connection: "
msgstr "Neočekávaná chyba při ověřování připojení: "

#: connections.py:122
msgid "Failed validation."
msgstr "Validace selhala."

#: connections.py:126
msgid "Access to Signal K server could not be checked."
msgstr "Přístup k serveru Signal K nelze zkontrolovat."

